    "swagger-ts": "^0.0.18",
    "method-override": "^2.3.6"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeautry/swagger-ts-example.git"
  }
